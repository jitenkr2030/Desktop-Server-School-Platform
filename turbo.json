{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "test": {
      "outputs": [],
      "dependsOn": []
    },
    "db:generate": {
      "dependsOn": ["^db:generate"]
    },
    "db:push": {
      "dependsOn": ["^db:generate"]
    },
    "db:studio": {
      "cache": false
    }
  },
  "globalDependencies": [
    ".env",
    ".env.local"
  ]
}
